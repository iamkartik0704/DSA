#include<bits/stdc++.h>
using namespace std;

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        if(nums.empty()) return 0;

        int j = 0; // write pointer for the next unique element

        for(int i = 1; i < nums.size(); i++) {
            if(nums[i] != nums[j]) {
                j++;             // move write pointer
                nums[j] = nums[i]; // assigning next values for further iterations
            }
        }

        return j + 1; // length of unique elements
    }
};
